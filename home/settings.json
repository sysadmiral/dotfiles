{
    "workbench.colorTheme": "Default Dark Modern",
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.defaultTimeFormat": null,
    "prettier.insertPragma": true,
    "dev.containers.defaultExtensions": [
        "eamodio.gitlens",
        "editorconfig.editorconfig",
        "esbenp.prettier-vscode"
    ],
    "window.menuBarVisibility": "toggle",
    "workbench.productIconTheme": "material-product-icons",
    "redhat.telemetry.enabled": false,
    "docker.languageserver.formatter.ignoreMultilineInstructions": true,
    "terminal.integrated.scrollback": 100000,
    "remote.SSH.defaultExtensions": [
        "googlecloudtools.cloudcode"
    ],
    "[python]": {
        "editor.formatOnType": true
    },
    "githubPullRequests.pullBranch": "never",
    "editor.mouseWheelScrollSensitivity": 0.1,
    "workbench.list.mouseWheelScrollSensitivity": 0.1,
    "workspacesort.workspaceDirectory": "/home/amo/code-workspaces/",
    "workbench.startupEditor": "none",
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.external.osxExec": "Warp.app",
    "security.promptForLocalFileProtocolHandling": false,
    "editor.renderControlCharacters": false,
    "markdown-preview-enhanced.liveUpdate": true,
    "markdown-preview-enhanced.enablePreviewZenMode": false,
    "workbench.iconTheme": "material-icon-theme",
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "geminicodeassist.project": "",
    "ansible.lightspeed.enabled": false,
    "ansible.lightspeed.suggestions.enabled": false,
    "docker.extension.enableComposeLanguageServer": false
}
